    "sayi_dizisi = [200, 7, 90, 800, 80]\n",
    "\n",
    "\n",
    "yeni_sayi_dizisi = []\n",
    "\n",
    "for sayi in sayi_dizisi:\n",
    "    if sayi < 100 :\n",
    "        yeni_sayi_dizisi.append(sayi +50)\n",
    "print(yeni_sayi_dizisi)\n"
   ]
